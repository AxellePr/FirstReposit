
 # нужно находиться в той же директории где находятся файлы
 # проверить это нужно командой "pwd"  перейти в нужную директорию командой "cd Start"
 # проверить файл нужно через терминал командой flake8 "имя файла.ру"
 [flake8]
exclude = __pycache__, env, venv
max-line-length = 200 # максимальная длина строки
max-complexity = # 8 степень вложенности
ignore = # игнорировать коды ошибок
E722,
E712,
E117,
F821,
E231,
E231,
